#!/bin/bash
set +H
set +o histexpand
source "$(dirname "${BASH_SOURCE[0]}")/lib/init"
[[ $BARE_OS == 'mac' ]] && export PATH="/opt/homebrew/bin:$PATH"

# if arguments are present, print them and exit
[[ -n $@ ]] && {

	[[ -e "b/$1" ]] && {
		scope="$1" && shift
		"b/$scope" "$@" && exit 0
	}

	echo "$@" && exit 0
}

exit 1